<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.FoodDeliveryApplication" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#006150</item>
        <item name="colorPrimaryVariant">#81006150</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#006150</item>
        <item name="colorSecondaryVariant">#006150</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <!--    <item name="android:windowSplashScreenIconBackgroundColor">#006150</item>-->
        <item name="android:windowBackground">@color/black</item>
<!--        <item name="android:windowSplashScreenAnimatedIcon">@drawable/appicon_light</item>-->

    </style>

    <string name="logo_img">@drawable/logo_light_full_size_removebg</string>
    <string name="icon_img">@drawable/appicon_dark_removebg</string>
    <string name="Login_page_logo">@drawable/round_rectangle_logo_img</string>

    <!--        <string name="app_logo">"@drawable/logo_light"</string>-->
    <style name="RatingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">#D3D3D3</item>
        <item name="colorControlActivated">#FFC922</item>
    </style>

    <style name="AlertDialogTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
    </style>
</resources>